<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent">

    <Text
        ohos:id="$+id:text_top"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:horizontal_center="true"
        ohos:above="$id:text_middle"
        ohos:text="top"
        ohos:text_size="20vp"
        />

    <Text
        ohos:id="$+id:text_middle"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:center_in_parent="true"
        ohos:text="middle"
        ohos:text_size="20vp"
        />

    <Text
        ohos:id="$+id:text_bottom"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:horizontal_center="true"
        ohos:text="bottom"
        ohos:below="$id:text_middle"
        ohos:text_size="20vp"
        />

    <Text
    ohos:id="$+id:text_topleft"
    ohos:height="match_content"
    ohos:width="match_content"
    ohos:align_parent_start="true"
    ohos:align_parent_top="true"
    ohos:text="topL"
    ohos:text_size="15vp"
    ohos:margin="40vp"
    />

    <Text
        ohos:id="$+id:text_topright"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:align_parent_end="true"
        ohos:align_parent_top="true"
        ohos:text="topR"
        ohos:text_size="15vp"
        ohos:margin="40vp"
        />

    <Text
        ohos:id="$+id:text_bottomleft"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:align_parent_left="true"
        ohos:align_parent_bottom="true"
        ohos:text="bottomL"
        ohos:text_size="15vp"
        ohos:margin="40vp"
        />

    <Text
        ohos:id="$+id:text_bottomright"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:align_parent_end="true"
        ohos:align_parent_bottom="true"
        ohos:text="bottomR"
        ohos:text_size="15vp"
        ohos:margin="40vp"
        />

    <Text
        ohos:id="$+id:text_left"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:align_parent_start="true"
        ohos:center_in_parent="true"
        ohos:text_alignment="start"
        ohos:text="left"
        ohos:text_size="15vp"
        ohos:margin="10vp"
        />

    <Text
        ohos:id="$+id:text_right"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:end_of="$id:text_middle"
        ohos:center_in_parent="true"
        ohos:align_parent_end="true"
        ohos:text="right"
        ohos:text_alignment="end"
        ohos:text_size="15vp"
        ohos:margin="10vp"
        />

</DependentLayout>